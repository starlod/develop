<VirtualHost *:80>
    DocumentRoot "<%= node["laravel"]["path"] %>/public"
    ServerName <%= node["laravel"]["servername"] %>
    ErrorLog <%= node["laravel"]["path"] %>/storage/logs/error_log
    TransferLog <%= node["laravel"]["path"] %>/storage/logs/access_log
    <Directory <%= node["laravel"]["path"] %>/public>
        Require all granted
        AllowOverride All
    </Directory>
</VirtualHost>
